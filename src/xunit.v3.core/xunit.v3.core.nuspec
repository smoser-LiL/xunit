<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>xunit.v3.core</id>
		<version>$PackageVersion$</version>
		<title>xUnit.net [Core Unit Testing Framework]</title>
		<authors>jnewkirk,bradwilson</authors>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">Apache-2.0</license>
		<licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
		<icon>content/logo-128-transparent.png</icon>
		<description>Includes the core library for writing tests using xUnit.net (xunit.v3.core.dll). Supports .NET Standard 2.0.</description>
		<copyright>Copyright (C) .NET Foundation</copyright>
		<repository type="git" url="https://github.com/xunit/xunit" commit="$GitCommitId$" />
		<dependencies>
			<group targetFramework=".NETCoreApp2.1">
				<dependency id="xunit.v3.extensibility.core" version="[$PackageVersion$]" />
				<dependency id="xunit.v3.runner.inproc.console" version="[$PackageVersion$]" />
			</group>
			<group targetFramework=".NETFramework4.7.2">
				<dependency id="xunit.v3.extensibility.core" version="[$PackageVersion$]" />
				<dependency id="xunit.v3.runner.inproc.console" version="[$PackageVersion$]" />
			</group>
			<!-- TODO: How do we support other runners than the in-process console runner? -->
			<group targetFramework=".NETStandard2.0">
				<dependency id="xunit.v3.extensibility.core" version="[$PackageVersion$]" />
			</group>
		</dependencies>
		<contentFiles>
			<files buildAction="None" copyToOutput="true" flatten="true" include="any\net472\xunit.v3.core.dll.tdnet" />
			<files buildAction="None" copyToOutput="true" flatten="true" include="any\net472\xunit.v3.runner.tdnet.dll" />
			<files buildAction="Compile" include="cs\net472\AutoGeneratedEntryPoint.cs" />
			<files buildAction="Compile" include="cs\netcoreapp2.1\AutoGeneratedEntryPoint.cs" />
			<files buildAction="Compile" include="fs\net472\AutoGeneratedEntryPoint.fs" />
			<files buildAction="Compile" include="fs\netcoreapp2.1\AutoGeneratedEntryPoint.fs" />
			<files buildAction="Compile" include="vb\net472\AutoGeneratedEntryPoint.vb" />
			<files buildAction="Compile" include="vb\netcoreapp2.1\AutoGeneratedEntryPoint.vb" />
		</contentFiles>
	</metadata>
	<files>
		<file target="build\" src="Package\build\*" />

		<file target="buildMultiTargeting\" src="Package\build\*" />

		<file target="content\" src="..\..\tools\media\logo-128-transparent.png" />

		<file target="contentFiles\any\net472\" src="bin\$Configuration$\netstandard2.0\xunit.v3.core.dll.tdnet" />
		<file target="contentFiles\any\net472\" src="..\xunit.v3.runner.tdnet\bin\$Configuration$\net472\merged\xunit.v3.runner.tdnet.dll" />
		<file target="contentFiles\cs\net472\" src="Package\content\*.cs" />
		<file target="contentFiles\cs\netcoreapp2.1\" src="Package\content\*.cs" />
		<file target="contentFiles\fs\net472\" src="Package\content\*.fs" />
		<file target="contentFiles\fs\netcoreapp2.1\" src="Package\content\*.fs" />
		<file target="contentFiles\vb\net472\" src="Package\content\*.vb" />
		<file target="contentFiles\vb\netcoreapp2.1\" src="Package\content\*.vb" />

		<file target="lib\net472\_._" src="..\build\_._" />
		<file target="lib\netcoreapp2.1\_._" src="..\build\_._" />
		<file target="lib\netstandard2.0\_._" src="..\build\_._" />
	</files>
</package>
